<?xml version="1.0"?>
<!DOCTYPE project [
<!ENTITY baseBuild SYSTEM "../org.homeunix.thecave.build/baseBuild.xml.inc">
]>
<project name="JSP Gallery" default="jar" basedir=".">

	<!-- Version number and other build-specific properties -->
	<property file="etc/build.properties"/>

	<!-- Set up all the project specific properties -->
	<property name="PROJECT_NAME" value="JSP Gallery"/>
	<property name="PROJECT_SHORT_NAME" value="moss-jsp-gallery"/>
	<property name="PROJECT_DESCRIPTION" value="Easily create photo galleries in JSP.  Uses Lightbox 2 to display images."/>
	<property name="PROJECT_COPYRIGHT" value="Copyright Wyatt Olson"/>

    <path id="classpath">
        <fileset dir="lib" includes="**/*.jar"/>
    	<fileset dir="WebContent/WEB-INF/lib" includes="**/*.jar"/>
    </path>
	
	&baseBuild;

</project>
